{
    "hosting": {
        "public": "build",
        "cleanUrls": true,

        "trailingSlash": false,

        "appAssociation": "AUTO",
        "rewrites": [{
            "source": "**",
            "destination": "/index.html"
        }],
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ]
    }
}